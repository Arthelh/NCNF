<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/organization_display_picture"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="54dp"
        app:layout_constraintBottom_toTopOf="@+id/organization_display_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/organization_display_name"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <TextView
        android:id="@+id/organization_display_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="51dp"
        android:text="PENISEMAIL"
        app:layout_constraintBottom_toTopOf="@+id/organization_display_postal"
        app:layout_constraintStart_toStartOf="@+id/organization_display_postal"
        app:layout_constraintTop_toBottomOf="@+id/organization_display_picture" />

    <TextView
        android:id="@+id/organization_display_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="126dp"
        android:text="PENIS"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/organization_display_postal"
        app:layout_constraintTop_toBottomOf="@+id/organization_display_postal" />

    <TextView
        android:id="@+id/organization_display_postal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="47dp"
        android:text="PENISPOSTAL"
        app:layout_constraintBottom_toTopOf="@+id/organization_display_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/organization_display_email" />

    <TextView
        android:id="@+id/organization_display_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="36dp"
        android:text="PENISNAME"
        app:layout_constraintBottom_toTopOf="@+id/organization_display_picture"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/organization_display_picture"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>